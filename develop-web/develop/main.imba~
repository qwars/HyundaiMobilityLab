
tag AsidePage < aside
	def render
		<self>

tag ArticlePage < section
	def render
		<self>

tag NavigationPage < nav

	def render
		<self>
			<aside>
			<ul>


export tag Sketch < main
	@classes = []

	def render
		<self>
			<NavigationPage route="/:collection/*:document*/*:field*">
			<ArticlePage route="/:collection/*:document*/*:field*">
			<AsidePage route="/:collection/*:document*/*:field*">